<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="SoF_SetCORSHeaders">
    <DisplayName>SoF_SetCORSHeaders</DisplayName>
    <Set>
        <Headers>
            <Header name="Access-Control-Allow-Origin">*</Header>
            <Header name="Access-Control-Allow-Methods">POST,OPTIONS</Header>
            <Header name="Access-Control-Allow-Headers">Accept, Authorization, Content-Type, If-None-Match</Header>
            <Header name="Access-Control-Max-Age">3628800</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>